                        <line id="Line_163" data-name="Line 163" y2="2.943" transform="translate(3.924 0)" fill="none"
                          stroke="#1e3a56" stroke-linecap="square" stroke-linejoin="round" stroke-width="1.5" />
                        <line id="Line_164" data-name="Line 164" x2="13.733" transform="translate(0 6.866)" fill="none"
                          stroke="#1e3a56" stroke-linecap="square" stroke-linejoin="round" stroke-width="1.5" />
                      </g>
                    </svg>

                  </div>
                </div>

              </div>

              <div class="input-field full_">
                <div class="inputWrap v2">
                  <div class="input-field">
                    <label for="cLang">اللغة المفضلة للشهادة</label>
                  </div>

                  <div class="input-field">
                    <div class="radioWrap v2 ">
                      <label>
                        <input name="group1" type="radio" id="cLang">
                        <span>عربي</span>
                      </label>
                      <label>
                        <input name="group1" type="radio">
                        <span>إنجليزي</span>
                        <label class="error">يتطلب حقلا</label>
                      </label>
