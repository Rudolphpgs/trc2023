{{> header}}
{{> loader}}
<main class="signupPage page">
  <section class="pageSection">
    <div class="container ">
      <div class="signupWrap pageWrap v2">
        <div class="contentBox finAnim">
          <h1 class=" ">
            <small>الصفحة الشخصية</small>
          </h1>
        </div>

        <div class="formBox finAnim d1">
          <form class="form-v3" action="{{ menuLink 'dashboard' }}">
            <div class="input_box">
              <div class="input-field">
                {{!-- <span class="form-text text-danger field-validation-error" data-valmsg-for="loginModel.Username"
                  data-valmsg-replace="true"><span id="loginModel_Username-error" class="">The Email Id field is
                    required.</span></span> --}}
                <label for="emailid">اميراتوس اميلويد</label>
                <input id="emailid" type="email" placeholder="example@name.com" readonly>
              </div>

              <div class="input-field">
                <label for="emailid">معرف البريد الإلكتروني TRC</label>
                <input id="emailid" type="email" placeholder="example@name.com" readonly>
              </div>

              <div class="inputWrap v3">
                <div class="input-field">
                  <label for="firstNameEng">الاسم الأول باللغة الإنجليزية</label>
                  <input id="firstNameEng" type="text">
                </div>

                <div class="input-field">
                  <label for="lastNameEng">الاسم الأخير باللغة الإنجليزية</label>
                  <input id="lastNameEng" type="text">
                </div>
              </div>

              <div class="inputWrap v3">
                <div class="input-field">
                  <label for="firstNameAr">الاسم الاول (باللغة العربية</label>
                  <input id="firstNameAr" type="text">
                </div>

                <div class="input-field">
                  <label for="lastNameAr">اسم العائلة بالعربي</label>
                  <input id="lastNameAr" type="text">
                </div>
              </div>

              <div class="inputWrap">
                <div class="input-field">
                  <label for="code">الرقم الدولي</label>
                  <select id="code" data-select disabled>
                    <option>+971</option>
                    <option>+91</option>
                    <option>+005</option>
                  </select>
                </div>

                <div class="input-field">
                  <label for="num">رقم الجوال الأساسي</label>
                  <input id="num" type="text">
                </div>
              </div>

              <div class="inputWrap">
                <div class="input-field">
                  <label for="code-02">الرقم الدولي</label>
                  <select id="code-02" data-select>
                    <option>+971</option>
                    <option>+91</option>
                    <option>+005</option>
                  </select>
                </div>

                <div class="input-field">
                  <label for="num-02">رقم الهاتف المحمول الثانوي</label>
                  <input id="num-02" type="text">
                </div>
              </div>

              <div class="input-field full_">
                <div class="titleSm">
                  <span>عنوان</span>
                </div>

              </div>

              <div class="input-field">
                <label for="emirates">الإمارات</label>
                <select id="emirates" data-select>
                  <option>أبو ظبي</option>
                  <option>دبي</option>
                  <option>الشارقة</option>
                  <option>عجمان</option>
                  <option>أم أل قون</option>
                  <option>راس أل خيمة</option>
                  <option>الفجيرة</option>
                </select>
              </div>

              <div class="input-field">
                <label for="area">منطقة</label>
                <input id="area" type="text" placeholder="Area here...">
              </div>

              <div class="input-field">
                <label for="streetName">اسم الشارع</label>
                <input id="streetName" type="text" placeholder="Street name here...">
              </div>

              <div class="input-field">
                <label for="buildingName">اسم المبنى</label>
                <input id="buildingName" type="text" placeholder="Building Name">
              </div>

              <div class="input-field">
                <label for="buildingNo">رقم المبنى</label>
                <input id="buildingNo" type="text" placeholder="Building Number">
              </div>

              <div class="input-field">
                <label for="landmark">معلم معروف</label>
                <input id="landmark" type="text" placeholder="Landmark name here...">
              </div>

              <div class="input-field">
                <label for="flatNo">رقم الفيلا / الشقة</label>
                <input id="flatNo" type="text" placeholder="Villa / Flat Number here...">
              </div>
            </div>
          </form>
        </div>

        <div class="moreWrap finAnim d2">
          <button type="submit" class="more lg_ red">
            <span>يلغي</span>
          </button>

          <a href="#otpVerify" type="submit" class="more lg_" data-fancybox>
            <span>يُقدِّم</span>
          </a>
        </div>
      </div>
    </div>
  </section>

</main>

<div id="otpVerify" class="popup popupBox sm_box" style="display: none;">
  <div class="formBox">
    <form class="form-v3">
      <div class="contentBox">
        <div class="textBox textCenter">
          <p>تحقق من عنوان بريدك الإلكتروني</p>
          <span>"تم إرسال OTP إلى example@company.com"</span>
        </div>
      </div>

      <div class="input_box">
        <div class="input-field full_">
          <span class="form-text text-danger field-validation-error" data-valmsg-for="loginModel.Username"
            data-valmsg-replace="true"><span id="loginModel_Username-error" class="">مطلوب حقل OTP.</span></span>
          <input id="otp" type="text" placeholder="Enter the OTP received in Email">
        </div>
      </div>

      <div class="moreWrap alCenter">
        <button type="submit" class="more lg_">
          <span>يُقدِّم</span>
        </button>
      </div>
    </form>
  </div>
</div>

{{> script-page}}
{{> footer}}